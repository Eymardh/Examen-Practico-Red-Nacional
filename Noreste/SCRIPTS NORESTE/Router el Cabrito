enable
configure terminal
hostname ElCabrito
no ip domain-lookup
ip domain-name juanmark.com
ipv6 unicast-routing

crypto key generate rsa modulus 1024
ip ssh version 2
username admin secret JuanMark123
line vty 0 4
 login local
 transport input ssh
exit

interface GigabitEthernet0/0
 description Link_a_MLS_Arriba
 ip address 148.60.0.1 255.255.255.252
 ipv6 address 2006:AFEA:B0CA:0::1/64
 ipv6 rip JUANMARK enable
 no shutdown
exit

interface GigabitEthernet0/1
 description Link_a_LaVaca
 ip address 10.10.10.1 255.255.255.252
 no shutdown
exit

interface GigabitEthernet0/2
 description Internet_ISP
 ip address dhcp
 no shutdown
exit

interface Serial0/0/0
 description Link_a_NubeCENTRO
 ip address 148.60.101.9 255.255.255.252
 no shutdown
exit

router rip
 version 2
 network 148.60.0.0
 network 10.0.0.0
 no auto-summary
 passive-interface GigabitEthernet0/2
exit

ipv6 router rip JUANMARK
exit
do write
